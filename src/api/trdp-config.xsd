      <xs:attribute name="offset" type="int32" use="optional">
        <xs:annotation>
          <xs:documentation>Offset for visualisation purposes (val = scale * x + offset).</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  
