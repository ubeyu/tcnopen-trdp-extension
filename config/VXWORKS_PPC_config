TARGET_VOS = vxworks
TARGET_FLAG = VXWORKS
TCPREFIX = 
TCPOSTFIX = ppc
DOXYPATH = /usr/local/bin/

CFLAGS += -O2 -Wall -DO_DC -fstrength-reduce -fno-builtin -mlongcall -fsigned-char -D$(TARGET_FLAG)  -lpthread -DB_ENDIAN